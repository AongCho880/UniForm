{
  "builds": [
    {
      "src": "uniform-frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "https://intelligent-success-production.up.railway.app/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
